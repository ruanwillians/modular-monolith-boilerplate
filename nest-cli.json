{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/agri-info-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/agri-info-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/agri-info-api",
  "projects": {
    "agro-api": {
      "type": "application",
      "root": "apps/agro-api",
      "entryFile": "main",
      "sourceRoot": "apps/agro-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agro-api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "businesss-exception": {
      "type": "library",
      "root": "libs/businesss-exception",
      "entryFile": "index",
      "sourceRoot": "libs/businesss-exception/src",
      "compilerOptions": {
        "tsConfigPath": "libs/businesss-exception/tsconfig.lib.json"
      }
    },
    "agri-info-api": {
      "type": "application",
      "root": "apps/agri-info-api",
      "entryFile": "main",
      "sourceRoot": "apps/agri-info-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agri-info-api/tsconfig.app.json"
      }
    },
    "exceptions": {
      "type": "library",
      "root": "libs/exceptions",
      "entryFile": "index",
      "sourceRoot": "libs/exceptions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/exceptions/tsconfig.lib.json"
      }
    },
    "postgres-database": {
      "type": "library",
      "root": "libs/postgres-database",
      "entryFile": "index",
      "sourceRoot": "libs/postgres-database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgres-database/tsconfig.lib.json"
      }
    }
  }
}